DEF SCRIPT Script {
   eventIn SFBool clicked
   eventOut SFColor color
   field SFBool lit FALSE
   url "vrmlscript:

function clicked(val) {
   if(val) {
      if(lit) { 
	 color = new SFColor(0,0,0); 
	 lit = FALSE;
      }
      else { 
	 color = new SFColor(1,1,0); 
	 lit = TRUE;
      }
   }
}"

}

ROUTE TOUCH.isActive TO SCRIPT.clicked
ROUTE SCRIPT.color TO li.color
ROUTE SCRIPT.color TO li1.color
 
